#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2009.vpi";
S_0x5d1f454efec0 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x5d1f454f0050 .scope module, "counter_0_999_tb" "counter_0_999_tb" 3 3;
 .timescale -9 -12;
v0x5d1f45506e00_0 .var "clk", 0 0;
v0x5d1f45506ed0_0 .net "count", 9 0, v0x5d1f45506c00_0;  1 drivers
v0x5d1f45506fa0_0 .var "reset", 0 0;
E_0x5d1f454c9640 .event anyedge, v0x5d1f45506c00_0;
S_0x5d1f454b5490 .scope module, "dut" "counter_0_999" 3 9, 4 1 0, S_0x5d1f454f0050;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /OUTPUT 10 "count";
v0x5d1f454b56e0_0 .net "clk", 0 0, v0x5d1f45506e00_0;  1 drivers
v0x5d1f45506c00_0 .var "count", 9 0;
v0x5d1f45506ce0_0 .net "reset", 0 0, v0x5d1f45506fa0_0;  1 drivers
E_0x5d1f454b5f20 .event posedge, v0x5d1f454b56e0_0;
    .scope S_0x5d1f454b5490;
T_0 ;
    %wait E_0x5d1f454b5f20;
    %load/vec4 v0x5d1f45506ce0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 10;
    %assign/vec4 v0x5d1f45506c00_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5d1f45506c00_0;
    %cmpi/e 999, 0, 10;
    %jmp/0xz  T_0.2, 4;
    %pushi/vec4 0, 0, 10;
    %assign/vec4 v0x5d1f45506c00_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x5d1f45506c00_0;
    %addi 1, 0, 10;
    %assign/vec4 v0x5d1f45506c00_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5d1f454f0050;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5d1f45506e00_0, 0, 1;
T_1.0 ;
    %delay 5000, 0;
    %load/vec4 v0x5d1f45506e00_0;
    %inv;
    %store/vec4 v0x5d1f45506e00_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x5d1f454f0050;
T_2 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5d1f45506fa0_0, 0, 1;
    %delay 20000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5d1f45506fa0_0, 0, 1;
    %pushi/vec4 3, 0, 32;
T_2.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_2.1, 5;
    %jmp/1 T_2.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
T_2.2 ;
    %load/vec4 v0x5d1f45506ed0_0;
    %pushi/vec4 999, 0, 10;
    %cmp/e;
    %flag_get/vec4 4;
    %cmpi/ne 1, 0, 1;
    %jmp/0xz T_2.3, 6;
    %wait E_0x5d1f454c9640;
    %jmp T_2.2;
T_2.3 ;
    %delay 20000, 0;
    %load/vec4 v0x5d1f45506ed0_0;
    %cmpi/ne 0, 0, 10;
    %jmp/0xz  T_2.4, 4;
    %vpi_call/w 3 29 "$display", "ERROR: Counter did not reset to 0 after reaching 999" {0 0 0};
    %vpi_call/w 3 30 "$finish" {0 0 0};
T_2.4 ;
    %jmp T_2.0;
T_2.1 ;
    %pop/vec4 1;
    %delay 1000000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5d1f45506fa0_0, 0, 1;
    %delay 10000, 0;
    %load/vec4 v0x5d1f45506ed0_0;
    %cmpi/ne 0, 0, 10;
    %jmp/0xz  T_2.6, 4;
    %vpi_call/w 3 39 "$display", "ERROR: Counter did not reset to 0 on reset signal" {0 0 0};
    %vpi_call/w 3 40 "$finish" {0 0 0};
T_2.6 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5d1f45506fa0_0, 0, 1;
    %delay 50000, 0;
    %vpi_call/w 3 46 "$display", "All tests passed!" {0 0 0};
    %vpi_call/w 3 47 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x5d1f454f0050;
T_3 ;
    %vpi_call/w 3 51 "$monitor", "Time: %0t, Reset: %b, Count: %d", $time, v0x5d1f45506fa0_0, v0x5d1f45506ed0_0 {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 5;
    "N/A";
    "<interactive>";
    "-";
    "counter_0_999_tb.v";
    "counter_0_999.v";
