    %load/vec4 v0x55a33c5b01c0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_11.4, 8;
    %vpi_call/w 3 58 "$display", "ERROR: counting should be low after timer completion" {0 0 0};
    %vpi_call/w 3 59 "$finish" {0 0 0};
